{
  // Editor
  "editor.fontSize": 19,
  "editor.letterSpacing": -1.0,
  "editor.minimap.size": "fit",
  "editor.minimap.showSlider": "always",
  "editor.formatOnPaste": true,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.wordWrap": "off",
  // "editor.wrappingColumn": 0,
  // "editor.wrappingIndent": "none",
  // "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "editor.fontFamily": "\"Fira Code\",Menlo, Monaco, 'Courier New', monospace",
  "editor.bracketPairColorization.enabled": true,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  // "editor.renderIndentGuides": false,
  // "editor.guides.indentation": true,
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.multiCursorModifier": "alt",
  "editor.largeFileOptimizations": false,
  "editor.renderControlCharacters": true,
  "editor.parameterHints.enabled": false,
  "editor.rulers": [79, 100],
  // "editor.linkedEditing": false,
  // Workbench
  "workbench.layoutControl.type": "menu",
  "workbench.editor.decorations.colors": true,
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.untitled.hint": "hidden",
  "sqltools.useNodeRuntime": true,
  // Terminal
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.letterSpacing": 0,
  "terminal.integrated.localEchoStyle": "dim",
  "terminal.integrated.fontFamily": "MesloLGS NF, Monospace",
  "terminal.integrated.cursorBlinking": true,
  //Degugger
  "debug.inlineValues": "off",
  // Vim
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.easymotion": true,
  "vim.smartRelativeLine": true,
  "vim.highlightedyank.enable": true,
  "keyboard.dispatch": "keyCode",
  "vim.handleKeys": {
    "<C-k>": false,
    "<C-b>": false,
    "<C-j>": false,
    "<C-w>": false,
    "<C-f>": false
  },
  // Other
  "security.workspace.trust.untrustedFiles": "open",
  "update.mode": "none",
  // Telemetry
  "telemetry.telemetryLevel": "off",
  "aws.telemetry": false,
  // "telemetry.enableCrashReporter": false,
  // "telemetry.enableTelemetry": false,
  "githubPullRequests.telemetry.enabled": false,
  "redhat.telemetry.enabled": false,
  "rest-client.enableTelemetry": false,
  "update.showReleaseNotes": false,
  // Markdown
  "markdown-preview-enhanced.enableScriptExecution": true,
  "markdown-preview-enhanced.chromePath": "/usr/bin/brave",
  // Python
  "python.pythonPath": "/usr/bin/python3",
  "python.languageServer": "Pylance",
  "python.autoComplete.addBrackets": true,
  "python.analysis.completeFunctionParens": true,
  "python.autoComplete.extraPaths": [
    "/usr/bin/python3" //Made by me
  ],
  "python.analysis.extraPaths": ["/usr/bin/python3"],
  "window.menuBarVisibility": "toggle",
  // Languages
  "[python]": {
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.occurrencesHighlight": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "omnisharp.path": "latest",
  "omnisharp.useGlobalMono": "always",
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  // Java
  "java.semanticHighlighting.enabled": true,
  // Jupyter
  "jupyter.askForKernelRestart": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.wordWrapColumn": 800,
  "python.formatting.provider": "black",
  "workbench.localHistory.enabled": false,
  "terraform.languageServer": {
    "external": true,
    "pathToBinary": "",
    "args": ["serve"],
    "ignoreSingleFileWarning": false
  },
  "terraform.codelens.referenceCount": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.diagnosticMode": "workspace",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.wrapTabs": true,
  "terminal.integrated.tabs.enabled": false,
  "workbench.editor.tabCloseButton": "off",
  "liveServer.settings.donotShowInfoMsg": true,
  "vsicons.presets.hideFolders": true,
  "python.linting.mypyEnabled": true,
  "editor.minimap.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "workbench.colorTheme": "GitHub Dark",
  "jupyter.interactiveWindow.textEditor.executeSelection": false,
  "reactSnippets.settings.importReactOnTop": false,
  "git.openRepositoryInParentFolders": "never",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoImportUserSymbols": true,
  "css.lint.unknownAtRules": "ignore"
}
